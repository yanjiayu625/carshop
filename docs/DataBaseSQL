团队绩效人员增减消息通知
DROP TABLE IF EXISTS `perf_team_adjust_report`;
CREATE TABLE `perf_team_adjust_report` (
 `id` INT(11) NOT NULL AUTO_INCREMENT,
 `event_id` INT(11) NOT NULL COMMENT '考核活动ID',
 `uid` varchar(50) NOT NULL DEFAULT '' COMMENT '负责人域账号',
 `name` varchar(100) NOT NULL DEFAULT '' COMMENT '团队名称',
 `dept_path` varchar(255) NOT NULL DEFAULT '' COMMENT '部门路径',
 `content` text NOT NULL COMMENT '内容',
 `status` tinyint(1) NOT NULL DEFAULT 1 COMMENT '状态: 1:是,2:否',
 `exe_uid` varchar(50) NOT NULL DEFAULT '' COMMENT '执行人Uid',
 `exe_name` varchar(100) NOT NULL DEFAULT '' COMMENT '处理人姓名',
 `exe_res` text NOT NULL COMMENT '处理内容',
 `c_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
 `u_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '修改时间',
 PRIMARY KEY (`id`)
) ENGINE=INNODB AUTO_INCREMENT=1 DEFAULT CHARSET=UTF8 COMMENT '团队绩效人员增减消息通知';

