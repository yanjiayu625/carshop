正太分布评分等级设置
DROP TABLE IF EXISTS `perf_ndist_score_info`;
CREATE TABLE `perf_ndist_score_info` (
 `id` INT(11) NOT NULL AUTO_INCREMENT,
 `name` varchar(50) NOT NULL DEFAULT '' COMMENT '等级名称',
 `desc` varchar(50) NOT NULL DEFAULT '' COMMENT '描述',
 `remarks` varchar(255) NOT NULL DEFAULT '' COMMENT '备注',
 `is_gt` tinyint(1) NOT NULL DEFAULT 1 COMMENT '大于等于,1:大于,2:大约等于',
 `gt_val`  tinyint(1) NOT NULL DEFAULT 1 COMMENT '大于的值',
 `is_lt` tinyint(1) NOT NULL DEFAULT 1 COMMENT '大于等于,1:小于,2:小约等于',
 `lt_val`  tinyint(1) NOT NULL DEFAULT 1 COMMENT '小于的值',
 `score_id` tinyint(1) NOT NULL DEFAULT 1 COMMENT '评分规则ID',
 `c_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
 PRIMARY KEY (`id`)
) ENGINE=INNODB AUTO_INCREMENT=1 DEFAULT CHARSET=UTF8 COMMENT '正太分布评分等级设置';


