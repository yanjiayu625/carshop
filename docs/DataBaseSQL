 PRIMARY KEY (`id`)
) ENGINE=INNODB AUTO_INCREMENT=1 DEFAULT CHARSET=UTF8 COMMENT '绩效考核最终等级';


绩效考核 历史考核指标内容
DROP TABLE IF EXISTS `perf_event_history_perf`;
CREATE TABLE `perf_event_history_perf` (
 `id` INT(11) NOT NULL AUTO_INCREMENT,
 `event_id` INT(11) NOT NULL COMMENT '考核活动ID',
 `uid` varchar(50) NOT NULL DEFAULT '' COMMENT '人员Uid',
 `name` varchar(50) NOT NULL DEFAULT '' COMMENT '人员姓名',
 `type` tinyint(1) NOT NULL DEFAULT 1 COMMENT '规则类型,1:正常,2:特殊',
 `lessue_id` varchar(50) NOT NULL DEFAULT '' COMMENT '指标排序',
 `lessue_wgt` varchar(50) NOT NULL DEFAULT '' COMMENT '指标权重',
 `lessue_name` varchar(255) NOT NULL DEFAULT '' COMMENT '指标名称',
 `lessue_content` text NOT NULL COMMENT '指标描述',
 `kpi_desc` text NOT NULL COMMENT '衡量标准',
 `c_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
 PRIMARY KEY (`id`)
) ENGINE=INNODB AUTO_INCREMENT=1 DEFAULT CHARSET=UTF8 COMMENT '绩效考核关系';


正太分布总体设置
DROP TABLE IF EXISTS `perf_ndist_info`;
CREATE TABLE `perf_ndist_info` (
 `id` INT(11) NOT NULL AUTO_INCREMENT,
 `is_director` tinyint(1) NOT NULL DEFAULT 1 COMMENT '是否总监级: 1:是,2:否',
 `type` tinyint(1) NOT NULL DEFAULT 1 COMMENT '规则类型,1:正常,2:特殊',
 `level` varchar(100) NOT NULL DEFAULT '' COMMENT '考核等级名称',
 `gle` tinyint(1) NOT NULL DEFAULT 1 COMMENT '逻辑原酸,1:等于,2:不超过,3:不小于',
 `prop` varchar(10) NOT NULL DEFAULT '' COMMENT '强制分布比例',
 `score_id` INT(11) NOT NULL COMMENT '评分规则ID',
 `c_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
 PRIMARY KEY (`id`)
) ENGINE=INNODB AUTO_INCREMENT=1 DEFAULT CHARSET=UTF8 COMMENT '正太分布总体设置';

