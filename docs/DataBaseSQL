 `dept_path` varchar(100) NOT NULL DEFAULT '' COMMENT '所属部门路径',
 `status` tinyint(1) NOT NULL DEFAULT 1 COMMENT '状态: 1:是,2:否',
 `c_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
 PRIMARY KEY (`id`)
) ENGINE=INNODB AUTO_INCREMENT=1 DEFAULT CHARSET=UTF8 COMMENT '团队绩效考核关系确认';

