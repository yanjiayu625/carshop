 `name` varchar(50) NOT NULL DEFAULT '' COMMENT '考核下级名称',
 `dept_path` varchar(100) NOT NULL DEFAULT '' COMMENT '考核下级部门路径',
 `score` varchar(20) NOT NULL DEFAULT '' COMMENT '考核分数',
 `level` varchar(20) NOT NULL DEFAULT '' COMMENT '考核等级',
