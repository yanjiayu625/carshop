    <script type="text/javascript">
        $(document).ready(function(){

            layui.use(['form','layer'], function(){
                var form = layui.form;
                var layer = layui.layer;
            })

            //全部
            $(document).on("click",".showfinance",function(){
                $("#all-finance").removeClass("layui-hide");
                $('.black_bg').show();
            });

            $(document).on("click",".close",function(){
                $("#all-finance").addClass("layui-hide");
                $(".yuyue-box").addClass("layui-hide");
                $('.black_bg').hide();
            });

            $(document).on("click",".black_bg",function(){
                $("#all-finance").addClass("layui-hide");
                $(".yuyue-box").addClass("layui-hide");
                $(".showtel-box").hide();
                $('.black_bg').hide();
                $("#configlist").hide();
            });


            //分期
            $('.top-instalment a').on('click', function(){
                $(".instalment-box").show();
                $('html').css({"overflow":"hidden"});
                $('body').css({"overflow":"hidden"});
            });

            $(document).on("click",".closeinstalment",function(){
                $(".instalment-box").hide();
                $('html').css({"overflow":"auto"});
                $('body').css({"overflow":"auto"});
            });

            $('.instalment-tab-title li').on('click', function(){
                var index = $(this).index();
                $(this).addClass("selected").siblings().removeClass("selected");
                $('.instalment-tab-content .paymentbox').eq(index).show().siblings().hide();
            });

            //贷款
            $('#loanbut').on('click', function(){
                $.get("/index.php?m=ajax&ajax=1&loan=1",{
                    pid : 243,
                    uid : 182
                }, function (data,textStatus){
                    var result = $.parseJSON(data);
                    if(result.code==1){
                        layer.msg(result.msg);
                    }
                    else if(result.code==2){
                        location.href="/login/";
                    }
                    else if(result.code==3){
                        layer.msg(result.msg);
                    }
                });
            });

            //预约
            $('.orderbtn').on('click', function(){
                $.get("/index.php?m=ajax&ajax=1&order=1",{
                    pid : 243,
                    uid : 182
                }, function (data,textStatus){
                    var result = $.parseJSON(data);
                    if(result.code==1){
                        layer.msg(result.msg);
                    }
                    else if(result.code==2){
                        location.href="/login/";
                    }
                    else{
                        layer.msg(result.msg);
                    }
                });
            });

            //砍价
            $('.bargainbtn').on('click', function(){
                $.get("/index.php?m=ajax&ajax=1&bargain=1",{
                    pid : 243,
                    uid : 182
                }, function (data,textStatus){
                    var result = $.parseJSON(data);
                    if(result.code==1){
                        layer.msg(result.msg);
                    }
                    else if(result.code==2){
                        location.href="/login/";
                    }
                    else{
                        layer.msg(result.msg);
                    }
                });
            });




            //收藏
            $('.collectbut').on('click', function(){
                var mythis = $(this);
                var textname = $(this).children('span').text();
                $.get("/index.php?m=ajax&ajax=1&collect=1",{
                    carid : 243
                }, function (data,textStatus){
                    if(data==1){
                        layer.msg('收藏成功！');
                        mythis.html("<i class='layui-icon'>&#xe67a;</i>取消");
                    }
                    else if(data==2){
                        layer.msg('请先登录', {time: 1000, icon: 1});
                        setTimeout(function(){//两秒后跳转
                            location.href = "/login";
                        },1000);
                    }
                    else{
                        layer.msg('取消收藏成功！');
                        mythis.html("<i class='layui-icon'>&#xe67b;</i>收藏");
                    }
                });
            });

            //拨打电话
            $(".dialing").click(function(){
                var mythis = $(this);
                var textname = $(this).text();
                $.get("/index.php?m=ajax&ajax=1&showtel=1",{
                    pid : 243
                }, function (data,textStatus){
                    var result = $.parseJSON(data);
                    if(result.code==1){
                        $('.black_bg').show();
                        $(".showtel-box").show();
                        $("#teltext").text(result.msg);
                    }
                    else if(result.code==2){
                        location.href="/login/";
                    }
                    else if(result.code==3){
                        layer.msg(result.msg);
                    }
                });
            });
            $("#boda").click(function(){
                var mobilephone = $("#teltext").text();
                location.href='tel://'+mobilephone;
            });



            $(".clickitem").click(function(){
                var idname = $(this).attr("data");
                var title = $(this).attr("title");
                $("#report").show();
                $("#reportinfo").html($('#'+idname).html());
                $("#title").text(title);
                $("#mainbox").hide();
                $(window).scrollTop(0);
            });

            $(document).on("click","#reback",function(){
                $("#mainbox").show();
                $("#report").hide();
            });

            $(document).on("click",".moreld",function(){
                $("#configlist").show();
                $(".black_bg").show();
            });

            $(document).on("click",".closeconfig",function(){
                $("#configlist").hide();
                $(".black_bg").hide();
            });

            $(document).on("click",".closecar",function(){
                if($("#cars", parent.document).length > 0) {
                    $("#cars", parent.document).hide();
                    $("#carsiframe", parent.document).attr('src','');
                    //$('html',parent.document).css({"overflow":"auto"});
                    $('body',parent.document).css({"overflow":"auto"});
                    $('body',parent.document).css({"padding-bottom":"50px"});
                }
                else{
                    history.go(-1);
                }
            });

            $(document).on("click",".clickimg",function(){
                var pswpElement = document.querySelectorAll('.pswp')[0];
                var items = [];
                // 点击后的详图
                items.push({src: 'http://ershouche.simcms.net/upload/image/20200914/20200914095317_47258.jpeg',w: 600,h: 400});
                items.push({src: 'http://ershouche.simcms.net/upload/image/20200914/20200914095317_71549.jpeg',w: 600,h: 400});
                items.push({src: 'http://ershouche.simcms.net/upload/image/20200914/20200914225414_71735.jpg',w: 600,h: 400});
                items.push({src: 'http://ershouche.simcms.net/upload/image/20200914/20200914225414_29382.jpg',w: 600,h: 400});

                // define options (if needed)
                var options = {
                    // optionName: 'option value'
                    // for example:
                    index: 0 // start at first slide
                };

                // Initializes and opens PhotoSwipe
                var gallery = new PhotoSwipe( pswpElement, PhotoSwipeUI_Default, items, options);
                gallery.init();
            });



        });
    </script>
