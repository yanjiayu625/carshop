    <script language="JavaScript">
        layui.use(['layer','flow'], function(){
            var layer = layui.layer;
            var $ = layui.jquery;
            var flow = layui.flow;
            flow.load({
                elem: '#listcar'
                ,done: function(page, next){
                    var lis = [];
                    $.get('/user.php?m=cars&a=list&isshow=1&ajax=1&page='+page, function(res){
                        layui.each(res.data, function(index, item){
                            lis.push('<li>'+ item +'</li>');
                        });
                        next(lis.join(''), page < res.pages);
                    },'json');
                }
            });
        });
        layui.use(['layer'], function(){
            var layer = layui.layer;
        });
        $(function() {
            //删除
            $(document).on("click",".del",function(){
                var thisitem = $(this);
                var id = parseInt(thisitem.attr('data-index'));
                $.get("/user.php?m=cars&a=del&id="+id,
                    function (data, textStatus){
                        var result = $.parseJSON(data);
                        if(result.code==1){
                            layer.msg(result.msg, {time: 1000, icon: 1});
                            thisitem.parent().parent('li').remove();
                        }
                        else{
                            layer.msg(result.msg, {time: 1000, icon: 2});
                        }
                    }
                );
            });

            //刷新
            $(document).on("click",".refresh",function(){
                var thisitem = $(this);
                var id = parseInt(thisitem.attr('data-index'));
                $.get("/user.php?m=cars&a=refresh&id="+id,
                    function (data, textStatus){
                        var result = $.parseJSON(data);
                        if(result.code==1){
                            layer.msg(result.msg, {time: 1000, icon: 1});
                            setTimeout(function(){//两秒后跳转
                                location.href = result.url;
                            },1000);
                        }
                        else{
                            layer.msg(result.msg, {time: 1000, icon: 2});
                        }
                    }
                );
            });

            //上下架
            $(document).on("click",".sell",function(){
                var thisitem = $(this);
                var id = parseInt(thisitem.attr('data-index'));
                $.get("/user.php?m=cars&a=sell&id="+id,
                    function (data, textStatus){
                        var result = $.parseJSON(data);
                        if(result.code==1){
                            if(result.msg=="取消下架成功"){
                                thisitem.html("下架");
                            }
                            else{
                                thisitem.html("上架");
                            }
                            thisitem.parent().parent('li').remove();
                            layer.msg(result.msg, {time: 1000, icon: 1});
                        }
                        else{
                            layer.msg(result.msg, {time: 1000, icon: 2});
                        }
                    }
                );
            });
        });
    </script>
