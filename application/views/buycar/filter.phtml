    <script language="JavaScript">
        $(function() {
            $(".onebrand").click(function(){
                var brandid = $(this).attr("data-index");
                $.get("/index.php?m=ajax&ajax=1&brandid="+brandid, {
                }, function (data, textStatus){
                    $("#brand-data").show();
                    $("#subbrand-data").html(data);
                    $("#brand-box").hide();
                });
            });
            $("#reback").click(function(){
                $("#brand-box").show();
                $("#brand-data").hide();
            });
            $(".option-item").click(function(){
                var value = $(this).attr('data');
                $(this).addClass('selected').parent().siblings().children().removeClass('selected');
                $(this).parent().parent().siblings('input').val(value);
            });
            $(".comfirm").click(function(){
                var con = "";
                var orderby = $("input[name=orderby]").val();
                if(orderby!="" && orderby!=0){
                    con += "s"+orderby;
                }
                var brand = $("input[name=brand]").val();
                if(brand!="" &&  brand!=0){
                    con += "b"+brand;
                }
                var subbrand = $("input[name=subbrand]").val();
                if(subbrand!="" && subbrand!=0){
                    con += "os"+subbrand;
                }
                var model = $("input[name=model]").val();
                if(model!="" && model!=0){
                    con += "oj"+model;
                }
                var price = $("input[name=price]").val();
                if(price!="" && price!=0){
                    con += "op"+price;
                }
                var age = $("input[name=age]").val();
                if(age!="" && age!=0){
                    con += "oa"+age;
                }
                var kilometre = $("input[name=kilometre]").val();
                if(kilometre!="" && price!=0){
                    con += "ok"+kilometre;
                }
                var transmission = $("input[name=transmission]").val();
                if(transmission!="" && transmission!=0){
                    con += "ot"+transmission;
                }
                var gas = $("input[name=gas]").val();
                if(gas!="" && gas!=0){
                    con += "og"+gas;
                }
                var emstandard = $("input[name=emstandard]").val();
                if(emstandard!="" && emstandard!=0){
                    con += "oe"+emstandard;
                }
                if(con!=""){
                    window.location.href="/buycar/list-"+con;
                }
                else{
                    window.location.href="/buycar/";
                }
            });
            $(".reset").click(function(){
                $("input").attr("value","");
                $(".option-item").removeClass('selected');
            });
        })
    </script>
